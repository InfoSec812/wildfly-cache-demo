<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.3"
                            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                            xsi:schemaLocation="urn:jboss:deployment-structure:1.3 https://www.jboss.org/schema/jbossas/jboss-deployment-structure-1_3.xsd">
  <deployment>
    <module-alias name="cache-wisely" />
    <dependencies>
      <module name="org.hibernate" services="import" />
      <module name="org.infinispan" services="import" />
      <module name="jakarta.persistence.api" services="import" />
      <module name="jakarta.servlet.api" services="import" />
      <module name="jakarta.servlet.jsp.api" services="import" />
      <module name="jakarta.servlet.jsp.api" services="import" />
      <module name="org.wildfly.extension.microprofile.smallrye-metrics" services="import" />
    </dependencies>
    <exclusions>
      <module name="org.jboss.resteasy.resteasy-json-binding-provider"/>
    </exclusions>
  </deployment>
</jboss-deployment-structure>
